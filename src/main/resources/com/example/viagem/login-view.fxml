<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.viagem.controller.LoginController">
   <children>
      <Label layoutX="224.0" layoutY="50.0" text="Login" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="450.0" AnchorPane.topAnchor="100.0">
         <font>
            <Font name="Arial Black" size="14.0" />
         </font>
      </Label>
      <Label layoutX="50.0" layoutY="122.0" text="Senha" AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="130.0">
         <font>
            <Font name="Arial Black" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="txf_login" layoutX="133.0" layoutY="99.0" />
      <Button fx:id="bt_entrarLogin" layoutX="124.0" layoutY="310.0" mnemonicParsing="false" onAction="#entrarLogin" text="Entrar">
         <font>
            <Font name="Arial Black" size="14.0" />
         </font>
      </Button>
      <Button fx:id="bt_sairLogin" layoutX="216.0" layoutY="310.0" mnemonicParsing="false" onAction="#sairLogin" text="Sair">
         <font>
            <Font name="Arial Black" size="14.0" />
         </font>
      </Button>
      <PasswordField fx:id="pf_senha" layoutX="133.0" layoutY="129.0" />
   </children>
</AnchorPane>
